<h1>Create an article</h1>

<%= form_for @article do |f| %>
    <!-- Paragraf för titel -->
    <p>
        <%= f.label :title %></br>
        <%= f.text_field :title %>
    </p>
    <!-- Paragraf för textinnehåll -->
    <p>
        <%= f.label :description %></br>
        <%= f.text_area :description%>
    </p>
    <!-- Paragraf för knappar -->
    <p>
        <%= f.submit %>
    </p>


<% end %>